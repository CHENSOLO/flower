<!-- {{res.title}} -->
<view data-post-id="{{res.postId}}" bind:tap="onGoToDetail">
<view class="post-author-date">
  <image catch:tap="OnMaxImage" class="post-author" src="{{res.avatar}}">11</image>
  <text class="post-date">{{res.date}}</text>
</view>
<text class="post-title">{{res.title}}</text>
<image class="post-image" src="{{res.imgSrc}}"></image>
<text class="post-content">{{res.detail}}</text>
<view class="post-like">
  <!-- <image class="post-like-image" src="/images/icon/chat.png"></image> -->
  <l-icon class="post-like-image" size="32" color="#666" name="favor"></l-icon>
  <text class="post-like-font">{{res.collection}}</text>
  <!-- <image class="post-like-image" src="/images/icon/view.png"></image> -->
  <l-icon class="post-like-image" size="32" color="#666" name="eye"></l-icon>
  <text class="post-like-font">{{res.postId}}</text>
</view>
</view>
